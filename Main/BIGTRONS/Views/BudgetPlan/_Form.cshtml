        blurArea = function (input) {
            var areaSize = App.Area.value == 0 ? 1 : App.Area.value;
            if (input.value <= 1) {
                App.treePanelBudgetVersionStructure.store.getNodeById('@BudgetPlanVM.Prop.AreaSize.Name').remove();
                App.treePanelBudgetVersionStructure.store.getNodeById('@BudgetPlanVM.Prop.BasicPriceI.Name').remove();
                App.treePanelBudgetVersionStructure.store.getNodeById('@BudgetPlanVM.Prop.BasicPriceII.Name').remove();
                App.treePanelBudgetVersionStructure.store.getNodeById('@BudgetPlanVM.Prop.BasicPriceIII.Name').remove();
            } else {
                console.log('in');

                nodeSubtot = App.treePanelBudgetVersionStructure.store.getNodeById('Subtotal');
                var Rootnode = App.treePanelBudgetVersionStructure.store.getRootNode();
                var valueGrandTotal = 0;
                for (var i = 0; i < Rootnode.childNodes.length - 2; i++) {
                    valueGrandTotal = valueGrandTotal + Rootnode.childNodes[i].data.total;
                }
                if (Ext.isEmpty(App.treePanelBudgetVersionStructure.store.getNodeById('@BudgetPlanVM.Prop.AreaSize.Name'))) {
                    node = nodeSubtot.clone();
                    console.log(node);
                    node.setId('@BudgetPlanVM.Prop.AreaSize.Name'),
                    node.data.itemdesc = '@Html.Raw(BudgetPlanVM.Prop.AreaSize.Desc)';
                    node.data.total = parseFloat(App.Area.value);
                    node.data.iconCls = "display: none !important;";
                    nodeSubtot.appendChild(node);
                }

                if (Ext.isEmpty(App.treePanelBudgetVersionStructure.store.getNodeById('@BudgetPlanVM.Prop.BasicPriceI.Name'))) {
                    node = new Object();
                    node = nodeSubtot.clone();
                    console.log(node);
                    node.setId('@BudgetPlanVM.Prop.BasicPriceI.Name'),
                    node.data.itemdesc = '@Html.Raw(BudgetPlanVM.Prop.BasicPriceI.Desc)';
                    node.data.total = valueGrandTotal / parseFloat(areaSize);
                    node.data.iconCls = "display: none !important;";
                    nodeSubtot.appendChild(node);
                }

                if (Ext.isEmpty(App.treePanelBudgetVersionStructure.store.getNodeById('@BudgetPlanVM.Prop.BasicPriceII.Name'))) {
                    node = new Object();
