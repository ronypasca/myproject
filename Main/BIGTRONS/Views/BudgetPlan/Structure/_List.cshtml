        var recalculatePrice= function(_node){
            while (_node!=null) {
                var _total=0;
                var _volume=0;
                var _material=0;
                var _wage =0;
                var _misc =0;

                for (var i = 0; i < _node.childNodes.length; i++) {
                    var total = (Ext.isEmpty(_node.childNodes[i].data.total)  ? 0: _node.childNodes[i].data.total);
                    _total+=total;

                    var material = (Ext.isEmpty(_node.childNodes[i].data.materialamount)  ? 0: _node.childNodes[i].data.materialamount);
                    _material+=material;

                    var wage = (Ext.isEmpty(_node.childNodes[i].data.wageamount)  ? 0: _node.childNodes[i].data.wageamount);
                    _wage+=wage;

                    var misc = (Ext.isEmpty(_node.childNodes[i].data.miscamount)  ? 0: _node.childNodes[i].data.miscamount);
                    _misc+=misc;


                }

                @*if(_node.get("isboi") && _node.parentNode.get("isboi") && _node.childNodes.length>0  ){
                    if( _node.childNodes[0].get("isboi")){
                        _node.set("@BudgetPlanVersionStructureVM.Prop.Total.Name.ToLower()",0);
                    }
                }else*@
                _node.set("@BudgetPlanVersionStructureVM.Prop.Total.Name.ToLower()",_total);

                var hasBOIChild=false;
                if(!Ext.isEmpty(_node.childNodes))
                    if(_node.childNodes[0].data.itemtypeid=="BOI")
                        hasBOIChild=true;

                if((_node.data.isboi && !hasBOIChild && _node.parentNode.data.isboi && _node.parentNode.id !="0")||
                    (_node.data.isahs)){
                    if (_material==0) {
                        _node.set("@BudgetPlanVersionStructureVM.Prop.MaterialAmount.Name.ToLower()","");
                    }else
                        _node.set("@BudgetPlanVersionStructureVM.Prop.MaterialAmount.Name.ToLower()",_material);

                    if (_wage==0) {
                        _node.set("@BudgetPlanVersionStructureVM.Prop.WageAmount.Name.ToLower()","");
                    }else
                        _node.set("@BudgetPlanVersionStructureVM.Prop.WageAmount.Name.ToLower()",_wage);

                    if (_misc==0) {
                        _node.set("@BudgetPlanVersionStructureVM.Prop.MiscAmount.Name.ToLower()","");
                    }else
                        _node.set("@BudgetPlanVersionStructureVM.Prop.MiscAmount.Name.ToLower()",_misc);

                    _volume = (Ext.isEmpty(_node.data.volume)  ? 0: _node.data.volume);

                    if(_volume>0){
                        var row_totalunitprice = (parseFloat(_material==null?0:_material)+parseFloat(_wage==null?0:_wage)+parseFloat(_misc==null?0:_misc));
                        var row_total =(parseFloat(_volume==null?0:_volume) * (Ext.isEmpty(row_totalunitprice)?0:row_totalunitprice));

                        if (row_total==0) {
                            _node.set("@BudgetPlanVersionStructureVM.Prop.TotalUnitPrice.Name.ToLower()","");
                            _node.set("@BudgetPlanVersionStructureVM.Prop.Total.Name.ToLower()","");
                        }else{
                            _node.set("@BudgetPlanVersionStructureVM.Prop.TotalUnitPrice.Name.ToLower()",row_totalunitprice);
                            _node.set("@BudgetPlanVersionStructureVM.Prop.Total.Name.ToLower()",row_total);
                        }
                    }


                }
                App.treePanelBudgetVersionStructure.store.getNodeById(_node.id).commit();
                _node=_node.parentNode;

            }
        };

