            var decTotal=0;
            decTotal = Ext.util.Format.number(((record.get == "total" || record.get("total") == null) ? 0 : record.get("total")), defaultNumberFormat);
            if(record.get("isboi") && record.parentNode.get("isboi") && record.childNodes.length>0  ){
                if( record.childNodes[0].get("isboi")){
                    decTotal= 0;
                    /*record.set("total",0);*/
                }
            }

            return (decTotal==0?null:decTotal.replace(/\.?0+$/, ''));
        };

