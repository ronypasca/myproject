        var getDeepAllChildNodes = function(node){
            var allNodes = new Array();
            if(node==null || node==undefined){
                return [];
            }

            if(!node.hasChildNodes()){
                return node;
            }else{
                allNodes.push(node);
                node.eachChild(function(Mynode){allNodes = allNodes.concat(getDeepAllChildNodes(Mynode));});
            }
            return allNodes;
        };


