@using com.SML.BIGTRONS.Enum;
@using com.SML.Lib.Common;
@{
    var X = Html.X();
    var FPTID = ViewData["FPTIDData"].ToString();

    List<Parameter> oParamList;
    Parameter oParam;
    oParamList = new List<Parameter>();
    oParam = new Parameter("FPTID", FPTID, ParameterMode.Value, false);
    oParamList.Add(oParam);
    oParam = new Parameter("GridList", "getAllGridData()", ParameterMode.Raw, true);
    oParamList.Add(oParam);
    Button.Builder btnSubmit = global_asax.Button(Buttons.ButtonSave, "BtnSaveWinner", "Save", "Save", true, false, false, null, oParamList, null, "");
    

}
@(btnSubmit)
@(X.Label().Html("<span class='legendbox' style='background-color:green;'>&nbsp;</span><i> Acceptable</i>").Cls("lblhigherinfo"))
@(X.Label().Html("<span class='legendbox' style='background-color:blue;' ;>&nbsp;</span><i> Below Standard Cost</i>").Cls("lblhigherinfo"))
@(X.Label().Html("<span class='legendbox' style='background-color:red;'>&nbsp;</span><i> Higher than Standard Cost</i>").Cls("lblhigherinfo"))
